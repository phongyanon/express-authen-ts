{
  "name": "express-authen-ts",
  "version": "1.0.0",
  "description": "",
  "main": "server.js",
  "scripts": {
    "build": "npx tsc",
    "start": "node dist/server.js",
    "dev:server": "nodemon --watch './**/*.ts' --exec 'ts-node' server.ts",
    "dev": "NODE_ENV=development ts-node server.ts",
    "test": "NODE_ENV=test jest --runInBand",
    "test:authen": "NODE_ENV=test jest --runInBand src/tests/authen.test.ts",
    "migrate:mysql": "NODE_ENV=development ts-node src/db_migration/mysql.ts",
    "migrate:mysql-test": "NODE_ENV=test ts-node src/db_migration/mysql.ts",
    "predev": "npm run swagger",
    "prebuild": "npm run swagger",
    "swagger": "tsoa spec"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "mysql2": "^3.6.1",
    "swagger-ui-express": "^5.0.0",
    "tsoa": "^5.1.1"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.3",
    "@types/cors": "^2.8.14",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.5",
    "@types/node": "^20.7.1",
    "@types/supertest": "^2.0.12",
    "@types/swagger-ui-express": "^4.1.5",
    "concurrently": "^8.2.2",
    "jest": "^29.7.0",
    "nodemon": "^3.0.1",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  }
